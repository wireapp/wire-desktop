  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/!(*.test*).ts": [
      "tslint --project tsconfig.json --fix",
      "git add"
    ],
    "**/*.test*.ts": [
      "tslint --project tsconfig.mocha.json --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "electron/dist/main.js",
  "name": "wire-desktop",
  "optionalDependencies": {
    "@wireapp/node-addressbook": "3.0.2"
  },
  "prettier": "@wireapp/prettier-config",
  "private": true,
  "productName": "WireInternal",
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-desktop.git"
  },
  "scripts": {
    "build:linux": "yarn build:prepare && wire-build-linux -w electron/wire.json",
    "build:linux:internal": "cross-env-shell APP_ENV=internal \"yarn configure && yarn build:linux\"",
    "build:macos": "yarn build:prepare && wire-build-macos -w electron/wire.json",
    "build:macos:internal": "cross-env-shell APP_ENV=internal \"yarn configure && yarn build:macos\"",
    "build:prepare": "yarn clear:wrap && yarn build:ts && yarn bundle",
    "build:ts": "yarn clear:ts && tsc",
    "build:ts:tests": "tsc -P tsconfig.mocha.json --noEmit",
    "build:win": "yarn build:prepare && wire-build-win -w electron/wire.json",
    "build:win:installer": "rimraf wrap/dist && wire-build-win-installer -w electron/wire.json",
    "build:win:internal": "cross-env-shell APP_ENV=internal \"yarn configure && yarn build:win\"",
    "bundle": "webpack --env.production",
    "bundle:dev": "webpack",
    "clear:coverage": "rimraf coverage",
    "clear:ts": "rimraf electron/dist",
    "clear:wrap": "rimraf wrap",
    "configure": "copy-config",
    "coverage": "yarn clear:coverage && nyc report",
    "fix": "yarn fix:js && yarn fix:other && yarn fix:ts && yarn fix:ts:tests",
    "fix:js": "yarn lint:js --fix",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "fix:ts:tests": "yarn lint:ts:tests --fix",
    "lint": "yarn lint:js && yarn lint:other && yarn lint:ts && yarn lint:ts:tests",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:ts:tests": "tslint --project tsconfig.mocha.json",
    "postinstall": "yarn configure && yarn rebuild",
    "prestart": "yarn build:ts && yarn bundle:dev",
    "prettier": "prettier \"**/*.{json,md,css}\"",
    "rebuild": "electron-rebuild -f -w @wireapp/node-addressbook",
    "start": "cross-env NODE_DEBUG=wire-desktop* electron . --inspect --devtools --enable-logging",
    "start:dev": "yarn start --env=https://wire-webapp-dev.zinfra.io",
    "start:edge": "yarn start --env=https://wire-webapp-edge.zinfra.io",
    "start:internal": "yarn start --env=https://wire-webapp-staging.wire.com",
    "start:localhost": "yarn start --env=http://localhost:8081",
    "start:prod": "yarn start --env=https://app.wire.com",
    "start:rc": "yarn start --env=https://wire-webapp-rc.zinfra.io",
    "test": "yarn lint && yarn build:ts && yarn build:ts:tests && yarn test:react && yarn test:main && yarn test:renderer && yarn coverage",
    "test:main": "electron-mocha --require .babel-register.js \"electron/src/**/*.test?(.main).ts\"",
    "test:renderer": "electron-mocha --renderer --require .babel-register.js \"electron/src/**/*.test?(.renderer).ts\"",
    "test:react": "jest"
  }
}
