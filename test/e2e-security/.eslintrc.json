{
  "extends": ["../../.eslintrc.json"],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "no-console": "off",
    "valid-jsdoc": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "no-unsanitized/property": "off",
    "import/no-unresolved": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["node_modules", "../../node_modules"]
      }
    }
  }
}
